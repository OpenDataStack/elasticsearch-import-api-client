{
    "name": "opendatastack/elasticsearch-import-api-client",
    "type": "library",
    "description": "API Client for the ElasticSearch Import API server",
    "keywords": ["elasticsearch", "kibana", "api", "client"],
    "homepage": "https://github.com/OpenDataStack/elasticsearch-import-api-client",
    "license": "GPL",
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "6.4.*"
    },
    "autoload": {
        "psr-4": {
          "OpenDataStack\\Client\\": "src/client/OpenDataStack/Client"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {

    }
}
